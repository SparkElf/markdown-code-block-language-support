{
	"name": "ts-for-markdown",
	"displayName": "ts for markdown",
	"description": "Embedded TS language server enables you to write JS, TS, JSX and TSX more smoothly.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"vscode-languageclient": "^8.0.1",
		"vscode-languageserver": "^8.0.1",
		"vscode-languageserver-textdocument": "^1.0.5"
	}
}